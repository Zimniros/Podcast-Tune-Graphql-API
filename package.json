{
  "name": "podcast-tune-backend",
  "version": "1.0.0",
  "description": "Graphql API server for PodcastTune, built with Node, Express, Apollo, Prisma, and PostgreSQL.",
  "main": "index.js",
  "repository": "git@github.com:Zimniros/Podcast-Tune-Graphql-API.git",
  "author": "Anton Zimnitski <zimniros@protonmail.com>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js --exec babel-node",
    "debug": "nodemon -e js,graphql -x node --inspect src/index.js --exec babel-node",
    "test": "jest",
    "playground": "graphql playground --dotenv .env",
    "deploy": "prisma deploy",
    "dev": "npm-run-all --parallel debug playground"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dayjs": "^1.8.15",
    "dotenv": "^8.0.0",
    "feedparser": "^2.2.9",
    "graphql": "^14.4.2",
    "graphql-cli": "^3.0.12",
    "graphql-yoga": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "nodemailer": "^6.3.0",
    "prisma": "^1.34.3",
    "prisma-binding": "^2.3.15"
  }
}
