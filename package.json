{
  "name": "podcast-tune-backend",
  "version": "1.0.0",
  "description": "Graphql API server for PodcastTune, built with Node, Express, Apollo, Prisma, and PostgreSQL.",
  "main": "index.js",
  "repository": "git@github.com:Zimniros/Podcast-Tune-Graphql-API.git",
  "author": "Anton Zimnitski <zimniros@protonmail.com>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js --exec babel-node",
    "debug": "nodemon -e js,graphql -x node --inspect src/index.js --exec babel-node",
    "test": "jest",
    "playground": "graphql playground --dotenv .env",
    "deploy": "prisma deploy",
    "dev": "npm-run-all --parallel debug playground"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^8.0.0",
    "graphql": "^14.2.1",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "^1.17.4",
    "lodash": "^4.17.11",
    "prisma": "^1.32.0",
    "prisma-binding": "^2.3.10"
  }
}
