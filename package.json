{
  "name": "podcast-tune-backend",
  "version": "1.0.0",
  "description": "Graphql API server for PodcastTune, built with Node, Express, Apollo, Prisma, and PostgreSQL.",
  "main": "index.js",
  "repository": "git@github.com:Zimniros/Podcast-Tune-Graphql-API.git",
  "author": "Anton Zimnitski <zimniros@protonmail.com>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "build": "babel src -d dist",
    "serve": "npm run build && node dist/index.js",
    "start": "npm run build && nodemon -e js,graphql src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect src/index.js --exec babel-node",
    "seed:local": "babel-node ./src/seed.js",
    "seed:prod": "node ./dist/seed.js",
    "delete": "prisma delete --force",
    "test": "jest",
    "deploy": "prisma deploy",
    "dev": "npm run debug"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dayjs": "^1.8.16",
    "dotenv": "^8.1.0",
    "feedparser": "^2.2.9",
    "graphql": "^14.5.4",
    "graphql-cli": "^3.0.14",
    "graphql-yoga": "^1.18.3",
    "jsdom": "^15.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "nodemailer": "^6.3.0",
    "prisma": "^1.34.8",
    "prisma-binding": "^2.3.15"
  }
}
