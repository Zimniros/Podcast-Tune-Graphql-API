type User {
  id: ID! @id
  name: String!
  email: String! @unique
}

type Category {
  id: ID! @id
  itunesId: String!
  name: String!
}

type Podcast {
  id: ID! @id
  itunesId: String!
  title: String!
  author: String!
  description: String!
  artwork: String!
  feedUrl: String!
  categories: [Category!]!
}
